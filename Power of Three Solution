class Solution(object):
    def isPowerOfThree(self, n):
        length = len(str(n))
        low_range = (2*length)-1
        high_range = (2*length)

        if n == pow(3, low_range) or n == pow(3, high_range):
            return True
        elif n == 1:
            return True
        else:
            return False
